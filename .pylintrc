[MASTER]
py-version=3.9

[MESSAGES CONTROL]
disable=raw-checker-failed,
        bad-inline-option,
        locally-disabled,
        file-ignored,
        suppressed-message,
        useless-suppression,
        deprecated-pragma,
        use-symbolic-message-instead,
        fixme,
        duplicate-code,
        too-many-arguments,
        too-few-public-methods,
        too-many-instance-attributes,
        import-error
